{

    "manifest_version": 2,
    "name": "Etran",
    "version": "1.0",
  
    "description": "Simple integrated translator",
  
    "icons": {
      "48": "icons/logo-48.png",
      "96": "icons/logo-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["etran.js"]
      }
    ],

    "permissions": [
        
    ],

    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "48": "icons/logo-48.png",
            "96": "icons/logo-96.png"
        },
        "default_title": "Etran",
        "default_popup": "/popup.html"
    }

    


  
  }